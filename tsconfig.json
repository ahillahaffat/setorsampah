{
  "compilerOptions": {
    "target": "esnext",
    "types": ["node", "bcryptjs"],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true, // Change this line
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "components/Navbar.tsx", "app/setorsampah/page.tsx", "app/layanan/page.tsx", "components/layanan/features-layanan.tsx", "components/layanan/setorsampahform.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
